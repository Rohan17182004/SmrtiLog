{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/", "dest": "main.py" },
    { "src": "/get_key", "dest": "main.py" },
    { "src": "/receive_logs", "dest": "main.py" },
    { "src": "/clear_logs", "dest": "main.py" }
  ]
}
